@model IEnumerable<AlgrmsWeb.Models.TaxForce>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header content-center">
    <h2>Index</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
</section>
<section class="content content-center">
    <div class="box box-primary">
        <div class="box-body">
            <div class="table table-responsive">
                <table class="table table-striped table-hover">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.issuer_code)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.task_force_code)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.task_force_name)
                        </th>
                       
                        <th>
                            @Html.DisplayNameFor(model => model.task_force_phone)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.task_force_email)
                        </th>
                        
                        <th>
                            @Html.DisplayNameFor(model => model.updated_at)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.issuer_code)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.task_force_code)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.task_force_name)
                            </td>
                            
                            <td>
                                @Html.DisplayFor(modelItem => item.task_force_phone)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.task_force_email)
                            </td>
                            
                            <td>
                                @Html.DisplayFor(modelItem => item.updated_at)
                            </td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { code = item.task_force_code }) |
                                @Html.ActionLink("Details", "Details", new { code = item.task_force_code }) |
                                @Html.ActionLink("Delete", "Delete", new { code = item.task_force_code })
                            </td>
                        </tr>
                    }

                </table>
            </div>
                
        </div>
    </div>
</section>
