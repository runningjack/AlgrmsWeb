@model IEnumerable<AlgrmsWeb.Models.Revenue>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="content-header content-center">
    <h1>
        Revenue List
    </h1>
    @Html.ActionLink("Create New", "Create", new { @class = "btn btn-primary", data_icon = "gear" })
</section>
<section class="content content-center">
    <div class="box box-primary">
        <div class="box-body">
            <div class="table table-responsive">
                <table class="table table-striped table-hover" id="tbl-items">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.category_code)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.revenue_code)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.revenue_name)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.description)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.frequency)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.amount)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.rate)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.created_at)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.updated_at)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.category_code)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.revenue_code)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.revenue_name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.description)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.frequency)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.amount)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.rate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.created_at)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.updated_at)
                            </td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { code = item.revenue_code }) |
                                @Html.ActionLink("Details", "Details", new { code = item.revenue_code }) |
                                @Html.ActionLink("Delete", "Delete", new { code = item.revenue_code })
                            </td>
                        </tr>
                    }

                </table>
</div>
        </div>
    </div>
</section>
